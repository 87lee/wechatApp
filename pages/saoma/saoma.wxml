    <view class="flex-item" style="border-left:1px solid #ccc;width:33%">类型</view>
    <view class="flex-item" style="border-left:1px solid #ccc;width:33%">内容</view>
  </view>
  <view  class="flex-wrp" style="flex-direction:row;display:flex;width:100%;border:1px solid #ccc;text-align:center"> 
    <view class="flex-item" style="width:33%">{{postData.errMsg}}</view>
    <view class="flex-item" style="border-left:1px solid #ccc;width:33%">{{postData.scanType}}</view>
    <view class="flex-item" style="border-left:1px solid #ccc;width:33%">{{postData.result}}</view>
  </view>
</view>


